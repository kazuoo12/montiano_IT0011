string = input("Enter a string: ")
vowels = consonants = spaces = others = 0
for char in string:
    if char.lower() in "aeiou":
        vowels += 1
    elif char.isalpha():
        consonants += 1
    elif char.isspace():
        spaces += 1
    else:
        others += 1
print(f"Vowels: {vowels}, Consonants: {consonants}, Spaces: {spaces}, Others: {others}")